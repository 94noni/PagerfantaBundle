{
    "name": "babdev/pagerfanta-bundle",
    "description": "Bundle to use Pagerfanta with Symfony2",
    "keywords": ["pagerfanta", "pagination", "symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "pagerfanta/pagerfanta": "^1.1 || ^2.0",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/property-access": "^3.4 || ^4.4 || ^5.0",
        "symfony/translation": "^3.4 || ^4.4 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.4 || ^5.0"
    },
    "require-dev": {
        "symfony/symfony": "^3.4 || ^4.4 || ^5.0",
        "phpunit/phpunit": "^3.7 || ^4.0 || ^5.0"
    },
    "conflict": {
        "twig/twig": "<1.34 || >=2.0,<2.4"
    },
    "autoload": {
        "psr-4": {
            "WhiteOctober\\PagerfantaBundle\\": ""
        },
        "exclude-from-classmap": ["Tests/", "TestsProject/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
