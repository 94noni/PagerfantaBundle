{
    "name": "babdev/pagerfanta-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to use Pagerfanta with Symfony2",
    "keywords": ["pagerfanta", "pagination", "symfony"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "pagerfanta/pagerfanta": "^1.1 || ^2.0",
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-foundation": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0",
        "symfony/property-access": "^3.4 || ^4.4 || ^5.0",
        "symfony/routing": "^3.4 || ^4.4 || ^5.0",
        "symfony/translation": "^3.4 || ^4.4 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.4 || ^5.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^8.5",
        "symfony/browser-kit": "^3.4 || ^4.4 || ^5.0",
        "symfony/console": "^3.4 || ^4.4 || ^5.0",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/yaml": "^3.4 || ^4.4 || ^5.0",
        "twig/twig": "^1.34 || ^2.4 || ^3.0"
    },
    "conflict": {
        "twig/twig": "<1.34 || >=2.0,<2.4"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\PagerfantaBundle\\": ""
        },
        "exclude-from-classmap": ["Tests/", "TestsProject/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev"
}
